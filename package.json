{
  "name": "marksense",
  "displayName": "Marksense",
  "description": "AI powered Intellisense",
  "version": "0.0.1",
  "publisher": "AlexWalchli",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/src/extension",
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "scripts": {
    "test": "standard 'src/**/*.js' 'test/**/*.js' --fix && mocha",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "standard": "^8.4.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "acorn": "^4.0.3"
  }
}
